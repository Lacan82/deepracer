<?xml version="1.0"?>
<launch>
  <arg name="world_name" />
  <arg name="kinesis_video_stream_name" />
  <arg name="kinesis_video_stream_region" default="us-east-1" />
  <arg name="include_second_camera" />
  <arg name="include_lidar_sensor" />
  <arg name="car_color" />
  <arg name="simapp_version"/>

  <include file="$(find deepracer_simulation)/launch/racecar.launch">
    <arg name="world_name" value="$(arg world_name)"/>
    <arg name="kinesis_video_stream_name" value="$(arg kinesis_video_stream_name)"/>
    <arg name="kinesis_video_stream_region" value="$(arg kinesis_video_stream_region)"/>
    <arg name="include_second_camera" value="$(arg include_second_camera)" />
    <arg name="include_lidar_sensor" value="$(arg include_lidar_sensor)" />
    <arg name="car_color" value="$(arg car_color)" />
    <arg name="simapp_version" value="$(arg simapp_version)" />
  </include>
</launch>
